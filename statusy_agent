-- ESP-12
-- dla agenta nr 168 dokonano 47 tys analiz, z czego 72% zostala wyplacona


with wniosek_analiza as
  (
  Select w.id, w.stan_wniosku, w.data_utworzenia, a.id_wniosku, a.status, a.data_utworzenia, a.id_agenta,
      s.kod_przyjazdu, s.kod_wyjazdu,
      count(a.id_agenta) as ilosc_analiz,
      count(case when a.status = 'odrzucony' then a.id_agenta end) as ilosc_odrzuconych,
      count(case when a.status = 'zaakceptowany' then a.id_agenta end) as ilosc_zaakceptowanych,
      count(case when w.stan_wniosku = 'wyplacony' then 1 end) over (partition by a.id_agenta)
      /
      count(w.id) over (partition by a.id_agenta)::numeric as proc_wypl_agent
  from wnioski w
  join analizy_wnioskow a on a.id_wniosku = w.id
  join podroze p on p.id_wniosku = w.id
  join szczegoly_podrozy s on s.id_podrozy = p.id
  where a.id_agenta is not null
  group by w.id, w.data_utworzenia, a.id_wniosku, a.status, a.data_utworzenia, a.id_agenta, s.kod_przyjazdu, s.kod_wyjazdu
  order by 8 desc
  )
Select id_agenta, count(id_agenta), proc_wypl_agent, kod_wyjazdu, kod_przyjazdu,
  ilosc_odrzuconych,
  ilosc_zaakceptowanych
from wniosek_analiza
group by id_agenta, ilosc_analiz, proc_wypl_agent, kod_przyjazdu, kod_wyjazdu, ilosc_odrzuconych, ilosc_zaakceptowanych
having count(id_agenta) > 1
order by 1 desc